{
  "name": "Fullbrain bookmark",
  "description": "Fullbrain bookmarking Extension",
  "version": "1.0",
  "manifest_version": 3,
   "key": "",
   "oauth2": {
    "client_id": "318840075254-7gehqn65jresfh8g6u77611h6mhj68cv.apps.googleusercontent.com",
    "scopes":[
      "profile email"
    ]
  },
  "background": {
    "service_worker": "./serviceworker/background.js"
  },
  "host_permissions": [
    "*://*.fullbrain.org/*"
  ],
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "tabs",
    "identity",
    "identity.email",
    "cookies",
    "storage"
    
  ],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "./icons/logo16.png",
      "32": "./icons/logo32.png",
      "48": "./icons/logo48.png",
      "128": "./icons/logo128.png"
    }
  },
  "icons": {
    "16": "./icons/logo16.png",
    "32": "./icons/logo32.png",
    "48": "./icons/logo48.png",
    "128": "./icons/logo128.png"
  }
}